{
  "compilerOptions": {
    "lib": ["DOM", "ES2021"],
    "module": "ES6",
    "target": "ES6",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "noImplicitOverride": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "noImplicitReturns": true
  }
}
